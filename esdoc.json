{
  "source": "./src",
  "destination": "./esdoc",
  "title": "SimpleDom-component",
  "index": "./DOC.md",
  "excludes": ["util\\.js", "converter\\.js"],
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {

        "replaces": [
          {
            "from": "^.*",
            "to": "dist/simpledom-component.js"
          }
        ]
      }
    },
    {
      "name": "esdoc-es7-plugin"
    }
  ]
}
